# proc ClockSetup  
  global  tbpath
  global  PathSeparator
  puts -nonewline "Setting up clock ... "
  if { [info exists PathSeparator] } { set ps $PathSeparator } else { set ps "/" }
  if { ![info exists tbpath] } { set tbpath "${ps}system" }
  isim force add $tbpath${ps}CLK_P 0 -time 0 ns -value 1 -time 10 ns -repeat 20 ns
  isim force add $tbpath${ps}CLK_N 1 -time 0 ns -value 0 -time 10 ns -repeat 20 ns
  puts "done"

# proc ResetSetup  
  global  tbpath
  global  PathSeparator
  puts -nonewline "Setting up reset and simulating ... "
  if { [info exists PathSeparator] } { set ps $PathSeparator } else { set ps "/" }
  if { ![info exists tbpath] } { set tbpath "${ps}system" }
  isim force add $tbpath${ps}RESET 0 -time 0 ns -value 1 -time 320 ns
  puts "done"

